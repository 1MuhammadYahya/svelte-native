    "tns-core-modules": "^5.4.3",
    "svelte": "^3.6.2"
  },
  "devDependencies": {
    "glob": "^7.1.3",
    "rollup": "^1.16.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.1.0",
    "rollup-plugin-typescript2": "^0.21.2",
    "svelte": "^3.6.2",
    "tns-core-modules": "^5.4.3",
    "typescript": "^3.5.2",
    "rimraf": "^2.6.3"
  }
}
