  },
  "devDependencies": {
    "glob": "^7.1.3",
    "rollup": "^1.1.2",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-typescript2": "^0.19.2",
    "svelte": "^3.0.0-beta.8",
    "svelte-loader": "^2.13.3",
    "tns-core-modules": "^5.2.1",
    "typescript": "^3.2.4",
    "rimraf": "^2.6.3"
  }
